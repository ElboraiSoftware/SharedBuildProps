{
	"$schema": "http://json.schemastore.org/template",
	"author": "Sam El-Borai (dgellow)",
	"name": "Elborai Software .NET project",
	"description": "A template for Elborai Software .NET projects",
	"identity": "TheTemplatedName",
	"shortName": "elboraisoftware",
	"tags": {
		"language": "C#",
		"type": "solution"
	},
	"sourceName": "TheTemplatedName",
	"symbols": {
		"usePrivateNugetPackages": {
			"type": "parameter",
			"datatype": "bool",
			"description": "Use ElboraiSoftware private NuGet packages",
			"default": false
		},
		"companyPrefix": {
			"type": "parameter",
			"datatype": "text",
			"description": "A company prefix used for all assemblies in the solution",
			"default": "ElboraiSoftware",
			"replaces": "TheTemplatedCompanyPrefix"
		},
		"solutionPrefix": {
			"type": "parameter",
			"datatype": "text",
			"description": "A solution prefix used for all assemblies in the solution",
			"default": "",
			"replaces": "TheTemplatedSolutionPrefix"
		},
		"copyrightYear": {
			"type": "generated",
			"description": "Date used for copyright",
			"datatype": "text",
			"generator": "now",
			"parameters": {
				"format": "yyyy"
			},
			"replaces": "TheTemplatedCopyrightYear"
		}
	},
	"guids": [
		"FAE04EC0-301F-11D3-BF4B-00C04F79EFBC",
		"3D74C28B-4A29-4C44-8AFB-10476225F79B",
		"A0513E77-269D-4CCB-95E1-3D0C1BBF3E4F"
	]
}